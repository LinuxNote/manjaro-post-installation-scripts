<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>Terminal</icon>
	<name>Terminal</name>
	<unique-id>1367866030392833-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>stock_folder</icon>
	<name>Thunar Root</name>
	<unique-id>1367866030392883-2</unique-id>
	<command>pkexec thunar %f</command>
	<description>Thunar Root</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>system-search</icon>
	<name>Search</name>
	<unique-id>1367866030392913-3</unique-id>
	<command>catfish %f</command>
	<description>find files and folders</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon></icon>
	<name>Copy file path</name>
	<unique-id>1893637941349276-1</unique-id>
	<command>echo -n &quot;%F&quot; | xclip -selection clipboard</command>
	<description>Copy file path with quotes to clipboard</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon></icon>
	<name>EXIF Metadata</name>
	<unique-id>1626637941359398-1</unique-id>
	<command>exiv2 -pa %f | GDK_SCALE=1 GTK_THEME=Matcha:dark zenity --width=1280 --height=720 --text-info --title=&quot;Exif Metadata&quot; --font=&quot;DejaVu Sans Mono&quot;</command>
	<description>Display tags in metadata</description>
	<patterns>*.jpg;*.jpeg;*.JPG</patterns>
	<image-files/>
</action>
<action>
	<icon>image-x-generic</icon>
	<name>Open with geeqie</name>
	<unique-id>9392037941312943-1</unique-id>
	<command>geeqie %f</command>
	<description>Open the selected image with geeqie</description>
	<patterns>*.jpg;*.jpeg;*.JPG;*.png;*.bmp;*.webp;*.gif</patterns>
	<image-files/>
</action>
</actions>
